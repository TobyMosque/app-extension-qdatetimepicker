  },
  "devDependencies": {
    "@quasar/app": "^2.1.14",
    "@quasar/quasar-app-extension-testing": "^1.0.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "majestic": "^1.2.24",
    "quasar-app-extension-qdatetimepicker": "link:../../",
    "strip-ansi": "=3.0.1",
    "wallaby-vue-compiler": "^1.0.3",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "engines": {
    "node": ">= 8.9.0",
